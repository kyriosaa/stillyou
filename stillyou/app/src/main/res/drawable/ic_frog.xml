<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="200dp"
    android:height="200dp"
    android:viewportWidth="200"
    android:viewportHeight="200">

    <!-- Head -->
    <path
        android:fillColor="#7CFC00"
        android:pathData="M50,80 Q20,120 50,160 Q100,200 150,160 Q180,120 150,80 Q100,40 50,80 Z" />

    <!-- Left Eye Bulge -->
    <path
        android:fillColor="#7CFC00"
        android:pathData="M60,50 Q50,30 60,20 Q70,30 65,50 Z" />

    <!-- Right Eye Bulge -->
    <path
        android:fillColor="#7CFC00"
        android:pathData="M140,50 Q150,30 140,20 Q130,30 135,50 Z" />

    <!-- Left Eye (converted from circle cx="65", cy="40", r="6") -->
    <path
        android:fillColor="#000000"
        android:pathData="M 65 40 m -6 0 a 6 6 0 1 0 12 0 a 6 6 0 1 0 -12 0" />

    <!-- Right Eye (converted from circle cx="135", cy="40", r="6") -->
    <path
        android:fillColor="#000000"
        android:pathData="M 135 40 m -6 0 a 6 6 0 1 0 12 0 a 6 6 0 1 0 -12 0" />

    <!-- Mouth -->
    <path
        android:strokeColor="#000000"
        android:strokeWidth="2"
        android:fillColor="@android:color/transparent"
        android:pathData="M80,140 Q100,160 120,140" />

    <!-- Cheeks -->
    <!-- Left Cheek (converted from circle cx="60", cy="130", r="6") -->
    <path
        android:fillColor="#FF9999"
        android:pathData="M 60 130 m -6 0 a 6 6 0 1 0 12 0 a 6 6 0 1 0 -12 0" />
    <!-- Right Cheek (converted from circle cx="140", cy="130", r="6") -->
    <path
        android:fillColor="#FF9999"
        android:pathData="M 140 130 m -6 0 a 6 6 0 1 0 12 0 a 6 6 0 1 0 -12 0" />
</vector>